~$ repo init -u git://github.com/CyanogenMod/android.git -b cm-10.1
~$ cp device/bn/acclaim/local_manifest.xml .repo/local_manifest.xml
~$ ./vendor/cm/get-prebuilts
~$ repo sync -d
~$  . build/envsetup.sh && brunch acclaim

